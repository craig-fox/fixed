    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-tape-runner": "^2.0.0",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-tap": "^1.1.0",
    "tap-notify": "0.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "watch-run": "^1.2.4",
    "yaml": "^0.2.3"
